{
	"manifest_version": 2,
	"name": "Avocalculator",
	"version": "1.0",
	"description": "Replaces money with the equivalent number of avocado toasts.",
	"options_page": "options.html",
	"permissions": [
        "storage"
    ],
    "icons": {
           "48": "icon48.png",
          "128": "icon128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

